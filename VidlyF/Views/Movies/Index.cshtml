@model VidlyF.ViewModels.MoviesCustomersViewModel
@{
	ViewBag.Title = "Movies";
	Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Movies</h2>

<table class="table	table-bordered	table-hover">
	<tr>
		<td>
			<b>Movie Name</b>
		</td>
	</tr>
	@foreach (Movie movie in Model.Movies)
	{
		<tr>
			<td>@movie.Name</td>
		</tr>
	}
</table>